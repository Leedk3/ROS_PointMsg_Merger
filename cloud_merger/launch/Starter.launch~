<launch>

  <!-- start node -->
  <node pkg="my_pcl_tutorial" type="my_pcl_tutorial" name="example"/>
  <!--fill out sensor info -->
  <arg name="nsensors" default="0" />
  <!-- sensor1 info start-->
  <arg name="s1_roll" default="0" />
  <arg name="s1_pitch" default="0" />
  <arg name="s1_yaw" default="0" />
  <arg name="s1_x" default="0" />
  <arg name="s1_y" default="0" />
  <arg name="s1_z" default="0" />
  <!-- sensor1 info end -->
  <!-- sensor2 info start-->
  <arg name="s2_roll" default="0" />
  <arg name="s2_pitch" default="0" />
  <arg name="s2_yaw" default="0" />
  <arg name="s2_x" default="0" />
  <arg name="s2_y" default="0" />
  <arg name="s2_z" default="0" />
  <!-- sensor2 info end -->
  <node pkg="nodelet" type="nodelet" name="driver_nodelet"
        args="load velodyne_driver/DriverNodelet velodyne_nodelet_manager" >
    <param name="model" value="$(arg model)"/>
    <param name="pcap" value="$(arg pcap)"/>
    <param name="read_once" value="$(arg read_once)"/>
    <param name="read_fast" value="$(arg read_fast)"/>
    <param name="repeat_delay" value="$(arg repeat_delay)"/>
    <param name="rpm" value="$(arg rpm)"/>
    <param name="frame_id" value="$(arg frame_id)"/>
  </node>    

</launch>
